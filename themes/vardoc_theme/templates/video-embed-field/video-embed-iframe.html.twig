{#
/**
 * @file
 * Display an iframe with alterable components.
 */
#}
{%
  set classes = [
    'embed-responsive-item',
  ]
%}
<iframe{{ attributes.addClass(classes) }}{% if url is not empty %} src="{{ url }}{% if query is not empty %}?{{ query | url_encode }}{% endif %}{% if fragment is not empty %}#{{ fragment }}{% endif %}"{% endif %}>&nbsp;</iframe>
