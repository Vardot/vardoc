{
  "name": "vardot/vardoc",
  "description": "Vardoc Knowledge Base",
  "type": "drupal-profile",
  "license": "GPL-2.0+",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Vardot",
      "homepage": "https://www.drupal.org/vardot",
      "role": "Maintainer"
    }
  ],
  "support": {
    "issues": "http://drupal.org/project/issues/vardoc",
    "source": "http://cgit.drupalcode.org/vardoc"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "package",
      "package": {
        "name": "kenwheeler/slick",
        "version": "1.6.0",
        "type": "drupal-library",
        "source": {
          "url": "https://github.com/kenwheeler/slick",
          "type": "git",
          "reference": "origin/master"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "dinbror/blazy",
        "version": "1.8.2",
        "type": "drupal-library",
        "source": {
          "url": "https://github.com/dinbror/blazy",
          "type": "git",
          "reference": "origin/master"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "ajaxorg/ace-builds",
        "version": "1.2.6",
        "type": "library",
        "source": {
          "url": "https://github.com/ajaxorg/ace-builds",
          "type": "git",
          "reference": "origin/master"
        }
      }
    }
  ],
  "require": {
    "php": ">=5.6",
    "composer/installers": "^1.2.0",
    "cweagans/composer-patches": "^1.6.0",
    "drupal-composer/drupal-scaffold": "^2.2.0",
    "drupal/drupal-library-installer-plugin": "~0.3",
    "mkalkbrenner/php-htmldiff-advanced": "~0.0.8",
    "enyo/dropzone": "4.3.0",
    "dinbror/blazy": "1.8.2",
    "kenwheeler/slick": "1.6.0",
    "vardot/varbase": "8.4.05",
    "drupal/social_api": "1.0-beta2",
    "drupal/social_auth": "1.0-beta4",
    "drupal/social_auth_google": "1.0-beta2",
    "drupal/fontawesome": "1.2",
    "drupal/default_content": "1.0-alpha6",
    "drupal/better_normalizers": "1.0-beta3",
    "drupal/menu_item_visibility": "1.x-dev"
  },
  "require-dev": {
    "drush/drush": "^8.0.0",
    "drupal/drupal-extension": "^3.2",
    "drupal-composer/drupal-scaffold": "^2.0.0",
    "jakoch/phantomjs-installer": "1.9.8",
    "behat/behat": "^3.0",
    "drupal/coder": "8.*",
    "phing/phing": "^2.14",
    "behat/mink": "~1.7",
    "behat/mink-goutte-driver": "~1.2",
    "behat/mink-zombie-driver": "@stable",
    "behat/mink-selenium2-driver":  "~1.3",
    "mikey179/vfsStream": "~1.2",
    "phpunit/phpunit": "~4.8",
    "guzzlehttp/guzzle": "6.2.*@stable",
    "symfony/css-selector": "~2.8",
    "squizlabs/php_codesniffer": "2.*",
    "behatch/contexts": "*",
    "emuse/behat-html-formatter": "@stable"
  },
  "config": {
    "bin-dir": "bin/",
    "secure-http": false
  },
  "autoload": {
    "psr-4": {
      "Vardoc\\composer\\": "src/composer"
    }
  },
  "scripts": {
    "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
    "pre-install-cmd": ["Vardoc\\composer\\ScriptHandler::checkComposerVersion"],
    "pre-update-cmd": ["Vardoc\\composer\\ScriptHandler::checkComposerVersion"],
    "post-install-cmd": [
      "Vardoc\\composer\\ScriptHandler::createRequiredFiles",
      "@composer drupal-scaffold",
      "./bin/phing push"
    ],
    "post-update-cmd": [
      "Vardoc\\composer\\ScriptHandler::createRequiredFiles",
      "./bin/phing push"
    ],
    "post-drupal-scaffold-cmd": ["Vardoc\\composer\\ScriptHandler::postDrupalScaffoldProcedure"]
  },
  "extra": {
    "installer-paths": {
      "docroot/core": ["drupal/core"],
      "docroot/profiles/{$name}": ["type:drupal-profile"],
      "docroot/modules/contrib/{$name}": ["type:drupal-module"],
      "docroot/themes/contrib/{$name}": ["type:drupal-theme"],
      "docroot/libraries/{$name}": ["type:drupal-library"]
    },
    "branch-alias": {
      "dev-8.x-1.x": "8.1.x-dev"
    },
    "drupal-libraries": {
      "library-directory": "docroot/libraries",
      "libraries": [
        {
          "name": "dropzone",
          "package": "enyo/dropzone"
        },
        {
          "name": "blazy",
          "package": "dinbror/blazy"
        },
        {
          "name": "slick",
          "package": "kenwheeler/slick"
        },
        {
          "name": "ace",
          "package": "ajaxorg/ace-builds"
        }
      ]
    }
  }
}
